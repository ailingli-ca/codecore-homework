<div class="'form-control">
    <label for="logoUrl" class="control-label">Logo Url</label>
    <input 
    type="text"
    class="form-control"
    name="logoUrl"
    placeholder="e.g. https://www.example.com/some.png"
    value="<%= cohort ? cohort.logoUrl : '' %>"
    >
  </div>
  <br>
  <div class="form-group">
    <label for="name" class="control-label">Name</label>
    <input 
    type="text"
    class="form-control"
    name="name"
    placeholder="Flying Funks"
    value="<%= cohort ? cohort.name : '' %>"
    >
  </div>
  <br>
  <div class="form-group">
    <label for="members" class="control-label">Members</label>
    <textarea  
    name="members"  
    class="form-control"  
    rows="5" placeholder="Jon Show, Daenerys, Delisandre, Arya Stark"
    ><%- cohort ? cohort.members : '' %>
    </textarea>
  </div>

  